{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "context": "local",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-copy-file",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:allow-exists",
    {
      "identifier": "fs:scope",
      "allow": [
        "$HOME/**/*",
        "$APPDATA/**/*"
      ]
    },
    "shell:allow-open",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    {
      "identifier": "http:default",
      "allow": [
        "https://dev.kittycad.io/*",
        "https://kittycad.io/*",
        "https://api.dev.kittycad.io/*"
      ]
    },
    "os:allow-platform",
    "os:allow-version",
    "os:allow-os-type",
    "os:allow-family",
    "os:allow-arch",
    "os:allow-exe-extension",
    "os:allow-locale",
    "os:allow-hostname"
  ],
  "platforms": [
    "linux",
    "macOS",
    "windows",
    "android",
    "iOS"
  ]
}