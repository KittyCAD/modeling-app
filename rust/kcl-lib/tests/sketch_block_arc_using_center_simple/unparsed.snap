---
source: kcl-lib/src/simulation_tests.rs
description: Result of unparsing sketch_block_arc_using_center_simple.kcl
---
@settings(experimentalFeatures = allow)

s = sketch(on = XY) {
  arc1 = sketch2::arc(start = [var 1mm, var 0], end = [var 0, var 2mm], center = [var 0, var 0])
arc1.start.at[0] == 1mm
arc1.start.at[1] == 0mm
arc1.end.at[0] == 0mm
  // arc1.end.at[1] not specified. Based on other constraints, it should solve.
arc1.center.at[0] == 0mm
arc1.center.at[1] == 0mm
}

assert(s.arc1.center.at[0], isEqualTo = 0)
assert(s.arc1.center.at[1], isEqualTo = 0)
assert(s.arc1.start.at[0], isEqualTo = 1mm)
assert(s.arc1.start.at[1], isEqualTo = 0)
assert(s.arc1.end.at[0], isEqualTo = 0)
assert(s.arc1.end.at[1], isEqualTo = 1mm)
