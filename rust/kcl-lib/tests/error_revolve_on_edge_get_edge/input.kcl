box = startSketchOn(XY)
  |> startProfile(at = [0, 0])
  |> line(end = [0, 10])
  |> line(end = [10, 0])
  |> line(end = [0, -10], tag = $revolveAxis)
  |> close()
  |> extrude(length = 10)

sketch001 = startSketchOn(box, face = revolveAxis)
  |> startProfile(at = [5, 10])
  |> line(end = [0, -10])
  |> line(end = [2, 0])
  |> line(end = [0, 10])
  |> close()
  |> revolve(axis = revolveAxis, angle = 90deg)
