---
source: kcl-lib/src/simulation_tests.rs
description: Result of unparsing sketch_block_var_substitution.kcl
---
@settings(experimentalFeatures = allow)

myParam = 4

s = sketch() {
  x = var 0
  y = var 10
x == 1 + 2
y == max([5 * myParam, 0])

  len = var 1in
len == 5mm

  p1 = [var, var]
p1[0] == 30
p1[1] == 40in
}

assert(s.x, isEqualTo = 3)
assert(s.y, isEqualTo = 20)
assert(s.len, isEqualTo = 5mm)
assert(s.p1[0], isEqualTo = 30)
// Should have converted to the module units (millimeters).
assert(s.p1[1], isEqualTo = 1016mm)
