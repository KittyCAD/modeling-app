@settings(defaultLengthUnit = in)

sketch001 = startSketchOn(-YZ)
profile001 = startProfile(sketch001, at = [-1, -1.16])
  |> xLine(length = 4.32)
  |> line(end = [-0.37, 3.38], tag = $seg01)
  // |> line(endAbsolute = [profileStartX(%), profileStartY(%)])
  |> close()
profile002 = startProfile(sketch001, at = [0.14, -0.84])
  |> line(end = [2.33, 2.13])
  |> line(end = [0.37, -2.03], tag = $yoyo)
  // |> line(endAbsolute = [profileStartX(%), profileStartY(%)])
  |> close()

out = subtract2d(profile001, tool = profile002)
extrude001 = extrude(out, length = 5)
