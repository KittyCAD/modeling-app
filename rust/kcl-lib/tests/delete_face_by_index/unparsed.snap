// Cylinder
cylinder = startSketchOn(XY)
  |> circle(center = [0, 0], radius = 4.09, tag = $seg01)
  |> extrude(length = 5)

// Tool
tool = startProfile(startSketchOn(XY), at = [0, -4.75])
  |> angledLine(angle = 0deg, length = 2, tag = $a)
  |> angledLine(angle = segAng(a) + 90deg, length = 9.5)
  |> angledLine(angle = segAng(a), length = -segLen(a))
  |> line(endAbsolute = [profileStartX(%), profileStartY(%)])
  |> close()
  |> extrude(length = 90)

twoParts = subtract(cylinder, tools = tool)
surface001 = deleteFace(twoParts, faceIndices = 1)
