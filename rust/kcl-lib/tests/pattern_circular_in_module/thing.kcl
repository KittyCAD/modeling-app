export fn thing() {
  exampleSketch = startSketchOn(XZ)
    |> startProfile(at = [.5, 25])
    |> line(end = [0, 5])
    |> line(end = [-1, 0])
    |> line(end = [0, -5])
    |> close()
    |> patternCircular2d(
         center = [0, 0],
         instances = 4,
         arcDegrees = 360deg,
         rotateDuplicates = true,
       )

  return extrude(exampleSketch, length = 1)
}

// This makes it run in isolated mode when the function is imported.
thing()
