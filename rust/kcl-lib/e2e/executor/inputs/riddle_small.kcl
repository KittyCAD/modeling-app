const ANSWER = 41803

fn t = (s) => {
  return (ANSWER * s + 12345) % 214748
}

let xs = 205804
let ys = 71816
  let ox = 35 - (t(xs) % 70)
  let oy = 35 - (t(ys) % 70)
  const r = startSketchOn('XZ')
    |> startProfileAt([ox, oy], %)
    |> line(end = [1, 0])
    |> line(end = [0, -1])
    |> line(end = [-1, 0])
    |> close()
    |> extrude(length = 1)
