// simple caster model at each corner
fn caster(originStart) {
  plane001c = {
      origin = [
        -(3.543 - 2.756) / 2 + originStart[0],
        -(3.543 - 2.756) / 2 + originStart[1],
        4.114
      ],
      x_axis = [1.0, 0.0, 0.0],
      y_axis = [0.0, 1.0, 0.0],
      z_axis = [0.0, 0.0, 1.0]
  }

  sketch001c = startSketchOn(plane001c)
    |> startProfile(at = [0, 0])
    |> xLine(length = 3.543)
    |> yLine(length = 3.543)
    |> xLine(length = -3.543)
    |> line(endAbsolute = [profileStartX(%), profileStartY(%)])
    |> close()
    |> subtract2d(tool = circle(center = [
         (3.543 - 2.756) / 2,
         (3.543 - 2.756) / 2
       ], radius = 8.8 / 2 / 25.4), %)
    |> subtract2d(tool = circle(center = [
         (3.543 - 2.756) / 2 + 2.756,
         (3.543 - 2.756) / 2
       ], radius = 8.8 / 2 / 25.4 ), %)
    |> subtract2d(tool = circle(center = [
         (3.543 - 2.756) / 2,
         (3.543 - 2.756) / 2 + 2.756
       ], radius = 8.8 / 2 / 25.4 ), %)
    |> subtract2d(tool = circle(center = [
         (3.543 - 2.756) / 2 + 2.756,
         (3.543 - 2.756) / 2 + 2.756
       ], radius = 8.8 / 2 / 25.4 ), %)
    |> extrude(length = -.25)

  sketch002c = startSketchOn(sketch001c, face = 'START')
    |> startProfile(at = [-originStart[0], 2.2 + originStart[1]])
    |> arc(
         angle_start = 30,
         angle_end = 330,
         radius = 3.2 / 2
       )
    |> close()
    |> extrude(length = 3.1)

  plane002c = {
      origin = [
        0 + originStart[0],
        0.8 + originStart[1],
        0
      ],
      x_axis = [1, 0, 0],
      y_axis = [0, 0, 1],
      z_axis = [1, 0, 0]
  }
  sketch003c = startSketchOn(plane002c)
    |> circle(center = [0, 1.2], radius = 2.48 / 2)
  examplec = extrude(sketch003c, length = -1 - (3 / 16))
  return examplec
}

