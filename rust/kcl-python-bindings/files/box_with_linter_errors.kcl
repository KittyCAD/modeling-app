// A 25x25x50 box

box_width = 25
box_depth = 25
box_height = 50

box_sketch = startSketchOn(XY)
  |> startProfile(at = [0, 0])
  |> xLine(length = box_width, tag = $line1)
  |> yLine(length = box_depth, tag = $line2)
  |> xLine(endAbsolute = profileStartX(%), tag = $line3)
  |> close(%, $line4)

box3D = extrude(box_sketch, length = box_height)