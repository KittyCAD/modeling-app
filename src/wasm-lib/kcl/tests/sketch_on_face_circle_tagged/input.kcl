fn cube = (pos, scale) => {
  sg = startSketchOn('XY')
    |> startProfileAt(pos, %)
    |> line([0, scale], %)
    |> line([scale, 0], %)
    |> line([0, -scale], %)

  return sg
}
part001 = cube([0, 0], 20)
  |> close(%)
  |> extrude(20, %)

part002 = startSketchOn(part001, "end")
  |> circle({ center = [0, 0], radius = 5 }, %, $myCircle)
  |> extrude(5, %)
