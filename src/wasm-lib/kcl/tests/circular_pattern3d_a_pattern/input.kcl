exampleSketch = startSketchOn('XZ')
  |> startProfileAt([0, 0], %)
  |> line([0, 2], %)
  |> line([3, 1], %)
  |> line([0, -4], %)
  |> close(%)
  |> extrude(1, %)

pattn1 = patternLinear3d({
  axis = [1, 0, 0],
  instances = 7,
  distance = 6
}, exampleSketch)

pattn2 = patternCircular3d({
  axis = [0, 0, 1],
  center = [-20, -20, -20],
  instances = 41,
  arcDegrees = 360,
  rotateDuplicates = false
}, pattn1)
