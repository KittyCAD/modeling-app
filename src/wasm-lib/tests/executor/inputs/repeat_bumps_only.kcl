w = 400

case = startSketchOn('XY')
    |> startProfileAt([-w, -w], %)
    |> lineTo([-w,  w], %)
    |> lineTo([ w, -w], %)
    |> lineTo([-w, -w], %)
    |> close(%)
    |> extrude(200, %)

bump1 = startSketchOn(case, 'end')
    |> circle({center: [-50, -50], radius: 40}, %)
    |> extrude(20, %)

// We pass in "bump1" here since we want to pattern just this object on the face.
|> patternLinear3d({
       axis: [1, 0, 0],
       instances: 3,
       distance: -100,
     }, %)
