fn square = (pos, scale) => {
  sg = startSketchOn('XY')
    |> startProfileAt(pos, %)
    |> line([0, scale], %)
    |> line([scale, 0], %)
    |> line([0, -scale], %)
    |> close(%)

  return sg
}

sq = square([0,0], 10)
cb = square([3,3], 4)
    |> extrude(10, %)

pt1 = sq.paths[0]
pt2 = cb.value[0]
